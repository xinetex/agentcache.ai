# AgentCache.ai Configuration

# Server
PORT=8787
NODE_ENV=production

# Redis (Upstash recommended)
REDIS_URL=rediss://default:YOUR_PASSWORD@your-redis.upstash.io:6379
UPSTASH_REDIS_URL=rediss://default:YOUR_PASSWORD@your-redis.upstash.io:6379

# Database (Neon PostgreSQL)
DATABASE_URL=postgresql://user:pass@host.neon.tech/agentcache?sslmode=require

# Stripe
STRIPE_SECRET_KEY=sk_live_...
STRIPE_PUBLISHABLE_KEY=pk_live_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Price IDs
STRIPE_PRICE_STARTER_MONTHLY=price_...
STRIPE_PRICE_STARTER_YEARLY=price_...
STRIPE_PRICE_PRO_MONTHLY=price_...
STRIPE_PRICE_PRO_YEARLY=price_...
STRIPE_PRICE_ENTERPRISE_MONTHLY=price_...
STRIPE_PRICE_ENTERPRISE_YEARLY=price_...

# JWT Secret
JWT_SECRET=your-super-secret-key-change-this

# SendGrid (optional - for email notifications)
SENDGRID_API_KEY=SG.xxx
SENDGRID_FROM_EMAIL=support@agentcache.ai

# Moonshot AI (Kimi K2) - optional for reasoning token caching
MOONSHOT_API_KEY=your_moonshot_api_key
MOONSHOT_ORG_ID=org-xxx
