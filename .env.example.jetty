# Add these to your .env file for JettyThunder integration

# JettyThunder API URL
JETTYTHUNDER_API_URL=http://localhost:3001  # Development
# JETTYTHUNDER_API_URL=https://jettythunder.app  # Production

# Webhook secret (shared between AgentCache and JettyThunder)
JETTYTHUNDER_WEBHOOK_SECRET=jetty_webhook_secret_change_me_in_production

# Internal webhook secret (for AgentCache internal calls)
INTERNAL_WEBHOOK_SECRET=internal_secret_change_me_in_production

# Database URL (for JettySpeed tables)
# DATABASE_URL=postgresql://user:pass@host/dbname  # Uncomment and set your Neon URL
