{
  "name": "agentcache-client",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript client for AgentCache.ai - Edge caching for AI API calls",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "echo \"Tests coming soon\" && exit 0",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "cache",
    "llm",
    "openai",
    "anthropic",
    "claude",
    "moonshot",
    "edge",
    "caching",
    "agent",
    "agentcache"
  ],
  "author": "AgentCache.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xinetex/agentcache.ai.git",
    "directory": "packages/client"
  },
  "bugs": {
    "url": "https://github.com/xinetex/agentcache.ai/issues"
  },
  "homepage": "https://agentcache.ai",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
