{
  "version": 2,
  "installCommand": "",
  "buildCommand": "",
  "outputDirectory": "public",
  "rewrites": [
    { "source": "/daily", "destination": "/daily.html" },
    { "source": "/visualizations", "destination": "/visualizations.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=300, must-revalidate" }
      ]
    }
  ]
}
